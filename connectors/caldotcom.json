{
  "id": "caldotcom",
  "name": "Cal.com",
  "description": "Cal.com open-source scheduling platform",
  "category": "Scheduling",
  "icon": "calendar",
  "color": "#292929",
  "version": "1.0.0",
  "authentication": {
    "type": "api_key",
    "config": {
      "type": "query",
      "name": "apiKey"
    }
  },
  "baseUrl": "https://api.cal.com/v1",
  "actions": [
    {
      "id": "test_connection",
      "name": "Test Connection",
      "description": "Test the connection to Cal.com",
      "parameters": {
        "type": "object",
        "properties": {},
        "required": [],
        "additionalProperties": false
      }
    },
    {
      "id": "list_bookings",
      "name": "List Bookings",
      "description": "List user bookings",
      "parameters": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "number",
            "description": "Filter by user ID"
          },
          "eventTypeId": {
            "type": "number",
            "description": "Filter by event type ID"
          },
          "status": {
            "type": "string",
            "enum": ["accepted", "pending", "cancelled", "rejected"],
            "description": "Filter by booking status"
          },
          "take": {
            "type": "number",
            "minimum": 1,
            "maximum": 100,
            "default": 10,
            "description": "Number of bookings to return"
          },
          "skip": {
            "type": "number",
            "description": "Number of bookings to skip"
          },
          "orderBy": {
            "type": "string",
            "enum": ["createdAt", "startTime", "endTime"],
            "description": "Order by field"
          },
          "sortOrder": {
            "type": "string",
            "enum": ["asc", "desc"],
            "description": "Sort order"
          }
        },
        "required": [],
        "additionalProperties": false
      }
    },
    {
      "id": "get_booking",
      "name": "Get Booking",
      "description": "Get a specific booking",
      "parameters": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "Booking ID"
          }
        },
        "required": ["id"],
        "additionalProperties": false
      }
    },
    {
      "id": "create_booking",
      "name": "Create Booking",
      "description": "Create a new booking",
      "parameters": {
        "type": "object",
        "properties": {
          "eventTypeId": {
            "type": "number",
            "description": "Event type ID"
          },
          "start": {
            "type": "string",
            "format": "date-time",
            "description": "Booking start time"
          },
          "end": {
            "type": "string",
            "format": "date-time",
            "description": "Booking end time"
          },
          "attendee": {
            "type": "object",
            "properties": {
              "name": {"type": "string"},
              "email": {"type": "string", "format": "email"},
              "timeZone": {"type": "string"}
            },
            "description": "Attendee information"
          },
          "meetingUrl": {
            "type": "string",
            "format": "uri",
            "description": "Meeting URL"
          },
          "title": {
            "type": "string",
            "description": "Booking title"
          },
          "description": {
            "type": "string",
            "description": "Booking description"
          },
          "location": {
            "type": "string",
            "description": "Meeting location"
          },
          "customFields": {
            "type": "object",
            "description": "Custom field values"
          }
        },
        "required": ["eventTypeId", "start", "end", "attendee"],
        "additionalProperties": false
      }
    },
    {
      "id": "update_booking",
      "name": "Update Booking",
      "description": "Update an existing booking",
      "parameters": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "Booking ID"
          },
          "title": {
            "type": "string",
            "description": "Booking title"
          },
          "description": {
            "type": "string",
            "description": "Booking description"
          },
          "start": {
            "type": "string",
            "format": "date-time",
            "description": "New start time"
          },
          "end": {
            "type": "string",
            "format": "date-time",
            "description": "New end time"
          },
          "status": {
            "type": "string",
            "enum": ["accepted", "pending", "cancelled", "rejected"],
            "description": "Booking status"
          }
        },
        "required": ["id"],
        "additionalProperties": false
      }
    },
    {
      "id": "cancel_booking",
      "name": "Cancel Booking",
      "description": "Cancel a booking",
      "parameters": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "Booking ID to cancel"
          },
          "reason": {
            "type": "string",
            "description": "Cancellation reason"
          }
        },
        "required": ["id"],
        "additionalProperties": false
      }
    },
    {
      "id": "list_event_types",
      "name": "List Event Types",
      "description": "List available event types",
      "parameters": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "number",
            "description": "Filter by user ID"
          },
          "take": {
            "type": "number",
            "minimum": 1,
            "maximum": 100,
            "default": 10,
            "description": "Number of event types to return"
          },
          "skip": {
            "type": "number",
            "description": "Number of event types to skip"
          }
        },
        "required": [],
        "additionalProperties": false
      }
    },
    {
      "id": "get_availability",
      "name": "Get Availability",
      "description": "Get user availability",
      "parameters": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "number",
            "description": "User ID"
          },
          "eventTypeId": {
            "type": "number",
            "description": "Event type ID"
          },
          "dateFrom": {
            "type": "string",
            "format": "date",
            "description": "Start date"
          },
          "dateTo": {
            "type": "string",
            "format": "date",
            "description": "End date"
          },
          "timeZone": {
            "type": "string",
            "description": "Timezone"
          }
        },
        "required": ["userId", "eventTypeId", "dateFrom", "dateTo"],
        "additionalProperties": false
      }
    }
  ],
  "triggers": [
    {
      "id": "booking_created",
      "name": "Booking Created",
      "description": "Triggered when a new booking is created",
      "type": "webhook",
      "parameters": {
        "type": "object",
        "properties": {
          "eventTypeId": {
            "type": "number",
            "description": "Filter by event type"
          }
        },
        "required": [],
        "additionalProperties": false
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "id": {"type": "number"},
          "title": {"type": "string"},
          "startTime": {"type": "string"},
          "endTime": {"type": "string"}
        }
      }
    },
    {
      "id": "booking_cancelled",
      "name": "Booking Cancelled",
      "description": "Triggered when a booking is cancelled",
      "type": "webhook",
      "parameters": {
        "type": "object",
        "properties": {},
        "required": [],
        "additionalProperties": false
      },
      "outputSchema": {
        "type": "object",
        "properties": {
          "id": {"type": "number"},
          "reason": {"type": "string"},
          "cancelledAt": {"type": "string"}
        }
      }
    }
  ],
  "testConnection": {
    "method": "GET",
    "endpoint": "/me"
  }
}