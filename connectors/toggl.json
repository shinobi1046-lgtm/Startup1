      }
    },
    {
      "id": "start_time_entry",
      "name": "Start Time Entry",
      "description": "Start a new time entry",
      "parameters": {
        "type": "object",
        "properties": {
          "workspace_id": {
            "type": "number",
            "description": "Workspace ID"
          },
          "description": {
            "type": "string",
            "description": "Time entry description"
          },
          "project_id": {
            "type": "number",
            "description": "Project ID"
          },
          "task_id": {
            "type": "number",
            "description": "Task ID"
          },
          "billable": {
            "type": "boolean",
            "default": false,
            "description": "Whether the time entry is billable"
          },
          "start": {
            "type": "string",
            "format": "date-time",
            "description": "Start time (ISO 8601 format)"
          },
          "duration": {
            "type": "number",
            "description": "Duration in seconds (for completed entries)"
          },
          "tags": {
            "type": "array",
            "items": {"type": "string"},
            "description": "List of tags"
          },
          "created_with": {
            "type": "string",
            "description": "Name of the client/application"
          }
        },
        "required": ["workspace_id"],
        "additionalProperties": false
      }
    },
